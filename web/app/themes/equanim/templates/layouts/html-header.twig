        <meta name="description" content="{{ site.description }}">
        <meta name="author" lang="fr" content="Julien Bredon">
        <link rel="apple-touch-icon" sizes="57x57" href="{{ site.theme.uri }}/assets/" />
        <link rel="apple-touch-icon" sizes="72x72" href="{{ site.theme.uri }}/assets/" />
